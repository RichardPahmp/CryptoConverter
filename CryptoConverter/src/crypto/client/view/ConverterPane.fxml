<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="CENTER" type="HBox" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="leftComboBox" prefWidth="150.0" />
      <TextField fx:id="leftTextField" onAction="#leftTextfieldAction"/>
      <TextField fx:id="rightTextField" onAction="#rightTextfieldAction"/>
      <ComboBox fx:id="rightComboBox" prefWidth="150.0" />
      <Button fx:id="closeButton" mnemonicParsing="false" text="C" onAction="#closeButtonClicked"/>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
